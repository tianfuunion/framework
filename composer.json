{
  "name": "tianfuunion/mark-engine",
  "description": "The Tianfu Union Mark Engine.",
  "keywords": [
    "tianfuunion",
    "markengine"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://mark.tianfu.ink",
  "authors": [
    {
      "name": "Mark Yount",
      "email": "mark@tianfuunion.cn"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-iconv": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0",
    "php-coveralls/php-coveralls": "^2.2",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "mark\\": "src"
    },
    "psr-0": {
      "": ""
    },
    "files": [
      "src/helper.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
